page: billboard, basic_frame
    module: java

    $local Ax1,Ay1,Ax2,Ay2: int
    $local Bx1,By1,Bx2,By2: int
    $local Cx1,Cy1,Cx2,Cy2: int

    &call scanner, "billboard.in"
        $map get_int, Ax1,Ay1,Ax2,Ay2
        $map get_int, Bx1,By1,Bx2,By2
        $map get_int, Cx1,Cy1,Cx2,Cy2
    $dump Ax1,Ay1,Ax2,Ay2

    $my int area
    area = (Ax2-Ax1)*(Ay2-Ay1)
    area += (Bx2-Bx1)*(By2-By1)

    $my w, h: int
    w = Math.max(0, Math.min(Ax2,Cx2)-Math.max(Ax1, Cx1))
    h = Math.max(0, Math.min(Ay2,Cy2)-Math.max(Ay1, Cy1))
    area -= w*h

    w = Math.max(0, Math.min(Bx2,Cx2)-Math.max(Bx1, Cx1))
    h = Math.max(0, Math.min(By2,Cy2)-Math.max(By1, Cy1))
    area -= w*h

    $dump area

    &call open_W, "billboard.out"
        $print $area
